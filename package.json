{
  "name": "@yolkai/slate-deep-table",
  "description": "A Slate plugin to handle tables containing complex content.",
  "version": "0.8.2",
  "license": "Apache-2.0",
  "repository": "git://github.com/jasonphillips/slate-deep-table.git",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "slate-html-serializer": "^0.7.20"
  },
  "peerDependencies": {
    "immutable": ">=3.8.1",
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "slate": "0.43.x || 0.44.x || 0.45.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^3.1.1",
    "eslint-config-gitbook": "1.4.0",
    "expect": "^1.20.2",
    "gh-pages": "^0.11.0",
    "http-server": "^0.9.0",
    "immutable": "^3.8.1",
    "mocha": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "slate": "^0.45.0",
    "slate-hyperscript": "^0.11.29",
    "slate-react": "^0.21.20",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "prepublish": "babel ./lib --out-dir ./dist",
    "postpublish": "npm run deploy-example",
    "lint": "eslint ./",
    "build-example": "browserify ./example/main.js -o ./example/bundle.js -t [ babelify ] ",
    "watch-example": "watchify ./example/main.js -o ./example/bundle.js  -t [ babelify ] ",
    "serve-example": "http-server ./example/ -p 8080",
    "start": "npm run build-example; npm run serve-example",
    "deploy-example": "npm run build-example; gh-pages -d ./example",
    "test": "./node_modules/.bin/mocha ./tests/*.js --compilers js:babel-register --reporter=list"
  },
  "keywords": [
    "slate"
  ]
}
